{
  "name": "reuse_io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"pnpm run dev:auth\" \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "dev:auth": "cd packages/auth && pnpm run dev",
    "dev:client": "cd packages/client && pnpm run dev",
    "dev:server": "cd packages/server && pnpm run dev",
    "build": "concurrently \"pnpm run build:auth\" \"pnpm run build:client\" \"pnpm run build:server\"",
    "build:auth": "cd packages/auth && pnpm run build",
    "build:client": "cd packages/client && pnpm run build",
    "build:server": "cd packages/server && pnpm run build",
    "start": "concurrently \"pnpm run start:auth\" \"pnpm run start:client\" \"pnpm run start:server\"",
    "start:auth": "cd packages/auth && pnpm run start",
    "start:client": "cd packages/client && pnpm run start",
    "start:server": "cd packages/server && pnpm run start"
  },
  "keywords": [],
